<style>
    .dropdown-menu {
        min-width: 120px !important;
    }

    .portlet.light.bordered {
        border: 0px !important;
    }

    .portlet.light {
        padding: 0px 20px 0px !important;
        background-color: #fff !important;
    }

    .tabbable-line > .nav-tabs > li.active {
        background: 0 0;
        border-bottom: 4px solid #36c6d3 !important;
    }
</style>

<script>

    $(document).ready(function () {

        var dxDate = @Html.Raw(Json.Encode(DateTime.Now.FromNtp().ToLongDateString()));
        @*var xssDate = @Html.Raw(Json.Encode(DateTime.Now.AddHours(11).FromNtp()));
        alert(xssDate);*@
        $('#ntpDate').html(moment(dxDate).format('dddd, D MMMM YYYY'));

    });

    moment.createFromInputFallback = function (config) { config._d = new Date(config._i); };

    $("body").on("click", "#previousday, #nextday, #_today,#_week, #_month", (function (e) {

        e.preventDefault();
        $('.page-container').loader({
            image: '../loader.gif'
        });

        var x = $('#ntpDate').text();
        var clicks = 0;

        clicks += 1;
        if (this.id == 'previousday') {
            var startdate = moment(x).add(-clicks, "days");
            $('#ntpDate').html(moment(startdate).format('dddd, D MMMM YYYY'));
        }

        if (this.id == 'nextday') {
            var startdate = moment(x).add(clicks, "days");
            $('#ntpDate').html(moment(startdate).format('dddd, D MMMM YYYY'));
        }

     if (this.id == '_today') {
            $("li.active").removeClass("active");
            $(this).closest('li').addClass('active');
            var startdate = @Html.Raw(Json.Encode(DateTime.Now.FromNtp().ToLongDateString()));
            $('#ntpDate').html(moment(startdate).format('dddd, D MMMM YYYY'));
        }

        if (this.id == '_week') {
            $("li.active").removeClass("active");
            $(this).closest('li').addClass('active');
            var startdate = @Html.Raw(Json.Encode(DateTime.Now.FromNtp().ToLongDateString()));
            var enddate = @Html.Raw(Json.Encode(DateTime.Now.FromNtp().AddDays(-7).ToLongDateString()));
            $('#ntpDate').html(moment(enddate).format('dddd, D MMMM YYYY') + ' - ' +  moment(startdate).format('dddd, D MMMM YYYY'));
        }


        if (this.id == '_month') {
            $("li.active").removeClass("active");
            $(this).closest('li').addClass('active');
            var startdate = @Html.Raw(Json.Encode(DateTime.Now.FromNtp().ToLongDateString()));
            var enddate = @Html.Raw(Json.Encode(DateTime.Now.FromNtp().AddMonths(-1).ToLongDateString()));
            $('#ntpDate').html(moment(enddate).format('dddd, D MMMM YYYY') + ' - ' +  moment(startdate).format('dddd, D MMMM YYYY'));
        }

        $.ajax({
            type: "POST",
            url: "/Dashboard/Next_Previous",
            data: JSON.stringify({ 'startdt': startdate, 'enddt': enddate }),
            contentType: "application/json",
            dataType: "text",
            cache: false,
            success: function (Result) {
                $("#divPartialView").html(Result);
                $('.page-container').loader('hide');
            },
            error: function (jqXHR, status, xhr) {
                (jqXHR.status && xhr.status == 404)
                alert(xhr.responseText); /* or your custom text*/
                $('.page-container').loader('hide');
            }
        });

        return false;
    }));


</script>



